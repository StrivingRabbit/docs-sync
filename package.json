{
	"name": "docs-sync-cli",
	"version": "1.0.2",
	"description": "A documentation synchronization tool for pulling content from multiple Git repositories or local directories and compiling them into your local project",
	"keywords": [
		"docs",
		"documentation",
		"sync",
		"markdown",
		"cli",
		"git",
		"include",
		"template",
		"multi-site"
	],
	"homepage": "https://github.com/StrivingRabbit/docs-sync#readme",
	"bugs": {
		"url": "https://github.com/StrivingRabbit/docs-sync/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/StrivingRabbit/docs-sync.git"
	},
	"license": "MIT",
	"author": "DCloud_Member <emxxme@qq.com>",
	"type": "commonjs",
	"main": "dist/src/config.js",
	"types": "dist/src/config.d.ts",
	"bin": {
		"docs-sync": "dist/bin/docs-sync.js"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"dev": "ts-node bin/docs-sync.ts",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"publish:patch": "npm version patch && npm publish",
		"publish:minor": "npm version minor && npm publish",
		"publish:major": "npm version major  && npm publish",
		"postpublish": "cnpm sync docs-sync-cli",
		"prepublishOnly": "npm run test:run && npm run build"
	},
	"dependencies": {
		"chokidar": "^3.6.0",
		"fast-glob": "^3.3.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	},
	"devDependencies": {
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^2.1.8",
		"vitest": "^2.1.8"
	},
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"types": "./dist/src/config.d.ts",
			"require": "./dist/src/config.js"
		}
	},
	"engines": {
		"node": ">=14.0.0"
	}
}
